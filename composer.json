{
    "name": "team8digital/trucker",
    "description": "A simple active resource implementation written in PHP to consume any REST API",
    "keywords":["trucker", "activeresource", "active resource"],
    "type": "library",
    "homepage": "https://team8digital.uk/",
    "license": "MIT",
    "authors": [
        {
            "name": "Team8Digital",
            "homepage": "https://team8digital.uk/",
            "email": "contact@team8digital.uk",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/brianwebb01/filesystem"
        },
        {
            "type": "vcs",
            "url": "https://github.com/brianwebb01/hot-phpunit-runner"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzle/guzzle": "~3.9",
        "illuminate/support": "*",
        "illuminate/config": "*",
        "illuminate/container": "*",
        "illuminate/filesystem": "*",
        "doctrine/inflector": "*",
        "patchwork/utf8": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "mockery/mockery": "0.9.*",
        "hot/phpunit-runner": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Trucker": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev"
}
