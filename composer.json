{
    "name": "team8digital/trucker",
    "description": "A simple active resource implementation written in PHP to consume any REST API",
    "keywords":["trucker", "activeresource", "active resource"],
    "type": "library",
    "homepage": "https://team8digital.uk/",
    "license": "MIT",
    "authors": [
        {
            "name": "Team8Digital",
            "homepage": "https://team8digital.uk/",
            "email": "contact@team8digital.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzle/guzzle": "3.8.*",
        "illuminate/support": "~4",
        "illuminate/config": "~4",
        "illuminate/container": "~4",
        "doctrine/inflector": "^1.2",
        "patchwork/utf8": "1.3.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Trucker\\": "src/Trucker"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trucker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "stable"
}
